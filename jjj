local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")

-- VÃ´ hiá»‡u hÃ³a anti-cheat dá»±a trÃªn hÃ m áº©n
local function DisableAntiCheat()
    for _, v in pairs(getgc(true)) do
        if type(v) == "function" and islclosure(v) then
            local info = debug.getinfo(v)
            if info and info.name and (info.name:lower():find("anticheat") or info.name:lower():find("bypass") or info.name:lower():find("detect")) then
                hookfunction(v, function(...) return nil end) -- Thay vÃ¬ delay, tráº£ vá» nil luÃ´n
            end
        end
    end
end

-- NgÄƒn cháº·n game gá»­i dá»¯ liá»‡u phÃ¡t hiá»‡n cheat lÃªn server
local function PreventDetection()
    local mt = getrawmetatable(game)
    setreadonly(mt, false)
    local oldNamecall = mt.__namecall
    mt.__namecall = newcclosure(function(self, ...)
        local args = {...}
        local method = getnamecallmethod()

        if method == "FireServer" or method == "InvokeServer" then
            local remoteName = tostring(self):lower()
            if remoteName:find("cheat") or remoteName:find("detect") or remoteName:find("log") then
                return nil -- KhÃ´ng gá»­i dá»¯ liá»‡u phÃ¡t hiá»‡n cheat
            end
        end
        return oldNamecall(self, ...)
    end)
end

-- Tá»± Ä‘á»™ng kÃ­ch hoáº¡t anti-cheat bypass khi menu báº­t
local function ActivateAntiCheatBypass()
    DisableAntiCheat()
    PreventDetection()
    print("âœ… Anti-Cheat Bypass Activated!")
end

ActivateAntiCheatBypass()




